{
  "name": "swagger-ui-cornify",
  "version": "1.0.3",
  "author": "",
  "description": "Cornify plugin for Swagger-UI v3",
  "main": "dist/SwaggerUICornify.js",
  "scripts": {
    "test": "exit 0",
    "build": "babel src --out-dir dist",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shockey/swagger-ui-cornify.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shockey/swagger-ui-cornify/issues"
  },
  "homepage": "https://github.com/shockey/swagger-ui-cornify#readme",
  "dependencies": {
    "cornified": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "release-it": "^3.1.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4",
            "browsers": "ie 11"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-es2015-modules-umd",
        {
          "globals": {
            "index.js": "SwaggerUICornify"
          },
          "moduleId": "SwaggerUICornify"
        }
      ]
    ]
  }
}
